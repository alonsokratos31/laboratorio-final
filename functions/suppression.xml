<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.2.xsd">
  
  <!-- Ignorar vulnerabilidad CVE específica -->
  <suppress>
    <cve>CVE-2023-12345</cve>
  </suppress>

  <!-- Ignorar cualquier hallazgo con CVSS < 5 -->
  <suppress>
    <cvssBelow>5.0</cvssBelow>
  </suppress>

  <!-- Ignorar una librería por CPE -->
  <suppress>
    <cpe>cpe:2.3:a:lodash:lodash:4.17.19:*:*:*:*:*:*:*</cpe>
  </suppress>

</suppressions>
